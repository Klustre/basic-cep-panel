<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ExtensionBundleId="com.klustre.basic" ExtensionBundleName="Basic Panel" ExtensionBundleVersion="1.0.0" Version="6.0">
  <ExtensionList>
    <Extension Id="com.klustre.basic.panel" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PHXS" Version="1.0" />
      <Host Name="IDSN" Version="1.0" />
      <Host Name="AICY" Version="1.0" />
      <Host Name="ILST" Version="1.0" />
      <Host Name="PPRO" Version="1.0" />
      <Host Name="PRLD" Version="1.0" />
      <Host Name="AEFT" Version="1.0" />
      <Host Name="FLPR" Version="1.0" />
      <Host Name="AUDT" Version="1.0" />
      <Host Name="DRWV" Version="1.0" />
      <Host Name="MUST" Version="1.0" />
      <Host Name="KBRG" Version="1.0" />
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="6.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.klustre.basic.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
          <CEFCommandLine>
            <Parameter>--allow-file-access-from-files</Parameter>
            <Parameter>--allow-file-access</Parameter>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Basic Panel</Menu>
          <Geometry>
            <Size>
              <Width>400</Width>
              <Height>350</Height>
            </Size>
          </Geometry>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
